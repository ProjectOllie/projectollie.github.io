			<script>
			(function () {
				var timer, $active, timer2;

				var clearActivePath = function () {
					$active && $active.removeClass("active");
				};

				var markActivePath = function ($target) {
					$active = $target.add($target.parents("nav li"));
					$active.addClass("active");
				}; 

				if(isMobile()) {

					($(document).on("touchstart", function (e) {
						console.log(e);
						var container = $("li.top.active");

						if (!container.is(e.target)
							&& container.has(e.target).length === 0) 
						{
							$("nav .active").removeClass("active");

						}
					}));

					($("nav")
						.on("touchend", "li", function (event) {
							$this = $(this);
							if(!$this.hasClass("active") && $this.has("ul").length) {
								event.preventDefault();
								$(this).addClass("active");
							}
						}));
				} else {
					($("nav")
						.on("mouseout", "li", function (event) {
							event.stopPropagation();
							clearTimeout(timer);

							timer2 = setTimeout(clearActivePath, 100);
						})
						.on("mouseover",  "li", function (event) {
							event.stopPropagation();
							clearTimeout(timer2);

							timer = setTimeout(function () {
								clearActivePath();	
								markActivePath($(this));					
							}.bind(this), 100);		
						})
						);	
				}
			}());

	var oldVal = '';
	$('#search-text-menu').on('change keypress paste focus textInput input', function() {
		var val = this.value;
		if (val !== oldVal) {
			oldVal = val;
			if(val.length >= 1)
			{
				$.get("/ajax/searchSite?text=" + val, function( data )
				{
					$('#search-results-menu').show();
					$('#search-results-menu').html(data);
				});
			} else {
				$('#search-results-menu').hide();
			}
		}
	});

	$('#menu-search-form').on('submit', function(e)
	{
		if($(this).attr('action').length > 0)
		{
			$(this).submit();
		} else {
			e.preventDefault();
		}
	});
	</script>

	</nav>
	<script type="text/javascript">
	var oldVal = '';
	var searchAjax = null;

	$('#search-text-input2').on('change keypress paste focus textInput input', function() {
		var val = this.value;
		var search = $('#search-type2').val();
		if (val !== oldVal) {
			oldVal = val;
			if(val.length >= 1)
			{
				if(searchAjax != null) searchAjax.abort();

				searchAjax = $.get("/ajax/searchSite?text=" + val + "&search=" + search, function( data )
				{
					$('#search-results-menu2').show();
					$('#search-results-menu2').html(data);
				});
			} else {
				$('#search-results-menu2').hide();
			}
		}
	});


	$('#search-text-input2').on('keypress', function(e) {
		if( e.which == 13 ) $('#search-results-menu2 .results a:first')[0].click();
	});


	$('#search-text-input2').on('keydown', function(e) {
		if( e.which == 27 ) $('#search-results-menu2').hide();
	});


	$(document).on('click',function(e){
		if( $(e.target).parents('.header-search').length != 1 ) $('#search-results-menu2').hide();
	});


	$('#search-text-input2').on('focus', function() {
		if( $('#search-text-input2').val().length > 0 ) $('#search-results-menu2').show();
	});

	$('#search-type2').on('change', function() {
		var val = $('#search-text-input2').val();
		var search = this.value;
		oldVal = val;
		if(val.length >= 1)
		{
			if(searchAjax != null) searchAjax.abort();

			searchAjax = $.get("/ajax/searchSite?text=" + val + "&search=" + search, function( data )
			{
				$('#search-results-menu2').show();
				$('#search-results-menu2').html(data);
			});
		} else {
			$('#search-results-menu2').hide();
		}
	});
	</script>
	<script type="text/javascript">
	function preload(arrayOfImages) {
		$(arrayOfImages).each(function(){
			$('<img/>')[0].src = this;
		});
	}
	preload([
		'/images/prime/badges/commend-1.png',
		'/images/prime/badges/commend-2.png',
		'/images/prime/badges/commend-3.png',
		'/images/prime/badges/commend-4.png',
		'/images/prime/badges/commend-5.png'
		]);
	var commendTarget;
	var commendType;
	var commendId;
	$(document).on('click', '.commend-popup', function(event) {
		event.preventDefault();
		commendTarget = '?commend=' + $(this).attr('data-id') + '&type=' + $(this).attr('data-type');
		commendType = $(this).attr('data-type');
		commendId = $(this).attr('data-id');

		$("#commendations-popup").show();
	});
	$(".commend-close").click(function(event) {
		event.preventDefault();
		$(".commendations-popup").hide();
	});
	$(".prime-upgrade-close").click(function(event) {
		event.preventDefault();
		$(".prime-upgrade-popup").hide();
	});

	$(document).on('click', '#use-commend', function(event)
	{
		event.preventDefault();
		$(this).prop('disabled', true);
		$(this).html('Commending...');

		$.ajax({url: '/ajax/commend' + commendTarget}).done(function( data ) {
			data = JSON.parse(data);
			if(data.success == false)
			{
				if(data.msg == 'redirect')
				{
					$(location).attr('href', '/purchase-commendations');
				} else {
					alert(data.msg);
				}
			} else {
				$("#commend-bank").html(parseInt($("#commend-bank").html() - 1));
				$("#memberMenuCommend").html(parseInt($("#memberMenuCommend").html() - 1));

				if(data.contentType == 'Build')
				{
					$("#CB" + commendType + commendId).children('#CBtooltip').html("Commend (+" + data.commend_count + ")");
				} else {
					$("#CB" + commendType + commendId).children('#CBtooltip').html("<span class='commendments tooltip' title='Number of Commendations'>(" + data.commend_count + ")</span>");
				}

				if(data.contentType == 'Comment') {
					var $lvl = $('#LevelComment' + commendId);
					var $badge = $('#BadgeComment' + commendId);
					var $user = $('#UserComment' + commendId);
					var $fade = $('#LevelComment' + commendId).find(".fade");
					$badge.show();

					$fade.fadeIn(function() {
						$lvl.removeClass('commend1 commend2 commend3')
						.addClass(data.commend_level);
						$fade.fadeOut("slow");
					});

					$badge.fadeOut(function() {
						($badge.removeClass('commend-badge1 commend-badge2 commend-badge3 commend-badge4 commend-badge5')
							.addClass(data.badge)
							.fadeIn("slow")
							);
					});

					$user.removeClass('prime1 prime2 prime3').addClass(data.user_flair);
				} else if(data.contentType == 'CommentExpanding') {
					var $badge = $('#BadgeComment' + commendId);
					var $user = $('#UserComment' + commendId);

					$badge.fadeOut(function(){
						$badge.html("<img class=\"ajax-tooltip { t:'CommendComment',i:'" + commendId + "' }\" src='" + data.badge + "' />").fadeIn('slow');
					});

					$('#CommendBorder' + commendId).addClass('commend');
					$user.removeClass('prime1 prime2 prime3').addClass(data.user_flair);

				} else if(data.contentType == 'Build') {
					var $badge = $("#CB" + commendType + commendId).children('#CBBadge');
					var $fade = $("#BuildFlames .fade");
					var $banner = $("#BuildFlames");

					$fade.fadeIn();
						// $banner.fadeOut(function() {
						// 	($banner.removeClass('prime lvl1 lvl2 lvl3')
						// 		.addClass(data.banner)
						// 		.fadeIn("slow")
						// 		);
						// 	$fade.fadeOut();
						// });
	$badge.fadeOut(function() {
		$badge.prop('src', data.badge).fadeIn("slow");
		$banner.removeClass('prime lvl1 lvl2 lvl3').addClass(data.banner);
		$fade.fadeOut();
	});
	} else if(data.contentType == 'BlogArticle') {
		if(data.commend_count == 1)
		{
			$('#BlogBadgeSpot').fadeOut(function(){
				$('#BlogBadgeSpot').html("<img id='BlogBadge" + commendId + "' src='" + data.badge + "' />").fadeIn('slow');
			});
		} else {
			$("#BlogBadge" + commendId).fadeOut(function(){
				$("#BlogBadge" + commendId).prop('src', data.badge).fadeIn("slow");
			});
		}
	}
	}

	$(".commendations-popup").hide();
	$('#use-commend').prop('disabled', false);
	$('#use-commend').html('Use (1) commendation');
	});
	});

	</script>
	<script type="text/javascript">
	var oldVal = '';
	var searchAjax = null;

	$('#search-text-input').on('change keypress paste focus textInput input', function() {
